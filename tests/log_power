#!/usr/bin/env expect-lite

*NOINFO
#*NOWARN
#*TIMESTAMP
*/\n/

; ============ WaterGuru $EL_pod $EL_testname: Connect to Power Supply $EL_power ============ 

>>picocom --baud 9600 --omap crcrlf --imap lfcrlf -r $EL_power
<<Terminal ready
>>*IDN?
<B&K Precision, 9201(.*)\n

$count=0
[ $count < 3
    >>MEAS:CURR?
    <[0-9](.*)\n
    :2
]     

